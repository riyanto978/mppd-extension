{
  "name": "DOM Reader",
  "version": "1.0",
  "manifest_version": 3,
  "description": "Reads the content of a page.",
  "permissions": [
    "scripting",
    "tabs"
  ],
  "host_permissions": [
    "https://admin.mppdigital.go.id/*",
    "https://sisdmk.kemkes.go.id/pencarian/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://admin.mppdigital.go.id/*",
        "https://sisdmk.kemkes.go.id/pencarian/*"
      ],
      "js": [
        "content_script.js",
        "sweetalert.min.js"
      ],
      "css": [
        "sweetalert.css"
      ]
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "commands": {
    "do-script": {
      "suggested_key": {
        "default": "Alt+B"
      },
      "description": "Inject a script on the page"
    }
  }
}